{
    "name": "fle/json-hydrator-bundle",
    "description": "This bundle is an alternative to an ORM like doctrine",
    "keywords": ["postgresql", "orm", "json", "hydrator", "serializer", "symfony"],
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Fabrice Lecomte",
            "email": "rusk23@gmail.com"
        }
    ],
    "require": {
        "php": "^7.2",
        "ext-ctype": "*",
        "ext-json": "*",
        "ext-pdo": "*",
        "ext-pdo_pgsql": "*",
        "jms/serializer-bundle": "3.1.0",
        "symfony/yaml": "4.2.*",
        "symfony/dependency-injection": "^4.0",
        "symfony/framework-bundle": "^4.0",
        "fle/json-hydrator": "1.0.*-dev"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.14",
        "phpunit/phpunit": "7.5.*",
        "symfony/browser-kit": "4.2.*",
        "symfony/css-selector": "4.2.*",
        "symfony/debug-bundle": "4.2.*",
        "symfony/stopwatch": "4.2.*",
        "symfony/var-dumper": "4.2.*",
        "symfony/web-profiler-bundle": "4.2.*"
    },
    "autoload": {
        "psr-4": {
            "FLE\\JsonHydratorBundle\\": ""
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "FLE\\JsonHydratorBundle\\Tests\\": "Tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "minimum-stability" : "dev",
    "prefer-stable": true
}
