{
    "name": "fle/json-hydrator-bundle",
    "description": "This bundle is an alternative to an ORM like doctrine",
    "keywords": ["postgresql", "orm", "json", "hydrator", "serializer", "symfony"],
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Fabrice Lecomte",
            "email": "fabrice.lecomte.be@gmail.com"
        }
    ],
    "require": {
        "php": "^7.4",
        "ext-ctype": "*",
        "ext-json": "*",
        "ext-pdo": "*",
        "ext-pdo_pgsql": "*",
        "jms/serializer-bundle": "^3",
        "symfony/yaml": "^5",
        "symfony/dependency-injection": "^5",
        "symfony/framework-bundle": "^5",
        "sensio/framework-extra-bundle": "v5.*",
        "fle/json-hydrator": "2.0.*-dev"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.14",
        "phpunit/phpunit": "7.5.*",
        "symfony/browser-kit": "^5",
        "symfony/css-selector": "^5",
        "symfony/debug-bundle": "^5",
        "symfony/stopwatch": "^5",
        "symfony/var-dumper": "^5",
        "symfony/web-profiler-bundle": "^5"
    },
    "autoload": {
        "psr-4": {
            "FLE\\JsonHydratorBundle\\": ""
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "FLE\\JsonHydratorBundle\\Tests\\": "Tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0-dev"
        }
    },
    "minimum-stability" : "dev",
    "prefer-stable": true
}
